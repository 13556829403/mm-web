<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.admin.web.mapper.UserMapper">

    <select id="login" parameterType="com.admin.web.form.LoginForm" resultType="com.admin.web.entity.User">
        select * from user where user_name = #{loginForm.userName} and pwd = #{loginForm.pwd}
    </select>

</mapper>